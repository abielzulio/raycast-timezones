{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "timezones",
  "title": "Timezones",
  "description": "Search and save worldwide timezone",
  "icon": "command-icon.png",
  "author": "abielzulio",
  "categories": [
    "Data",
    "Communication",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Timezone",
      "description": "Search and save worldwide timezone",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "isHour12",
      "description": "Enable AM/PM time format",
      "type": "checkbox",
      "label": "Enable AM/PM time format",
      "title": "AM/PM",
      "required": false
    }
  ],
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@raycast/api": "^1.45.2",
    "@raycast/utils": "^1.4.14",
    "@types/sqlite3": "^3.1.8",
    "papaparse": "^5.3.2",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/papaparse": "^5.3.5",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
